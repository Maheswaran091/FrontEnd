        const data = await resp.json();
        const filterSection = document.querySelector("#plugins-list");
        if (!data || !data.products || !filterSection || document.querySelector(".presets-plugins")) return;
        const presetsSection = createPresetsSection(data);
        filterSection.insertAdjacentHTML("afterend", presetsSection);
        updateFilterCount();
        // Add filter listeners after rendering
        addFilterListeners();
        // Initialize Show More functionality on page load
        const allProducts = Array.from(document.querySelectorAll("#search_results_presets .c-product__wrapper"));
        const showMoreBtn = document.querySelector(".loadmoreBtn2");
        updateProductVisibility(allProducts, showMoreBtn);
      } catch (error) {
        console.log(error);
      }
    }

    // Utility: Get unique filter values from products
    function getUniqueFilterValues(products) {
      const genres = new Set();
      const instruments = new Set();
      const categories = new Set();
      const plugins = new Set();
      products.forEach(product => {
        if (product.genre) {
          product.genre.split(";").forEach(g => genres.add(g.trim()));
        }
        if (product.instrument) {
          product.instrument.split(";").forEach(i => instruments.add(i.trim()));
        }
        if (product.category) {
          categories.add(product.category.trim());
        }
        if (product.plugin) {
          product.plugin.split(";").forEach(p => plugins.add(p.trim()));
        }
      });
      return {
        genres: Array.from(genres).sort(),
        instruments: Array.from(instruments).sort(),
        categories: Array.from(categories).sort(),
        plugins: Array.from(plugins).sort(),
      };
    }

    // Helper: Render a filter column
    function renderFilterColumn(title, values, filterKey) {
      // Distribute values in column-fill order
      const col1 = [];
      const col2 = [];
      values.forEach((val, i) => {
        if (i % 2 === 0) col1.push(val);
        else col2.push(val);
      });

      return `
        <div class="filter-col">
          <div class="filter-title">${title}</div>
          <div class="filter-list" style="display: flex; gap: 16px;">
            <div style="display: flex; flex-direction: column; gap: 4px;">
              ${col1
                .map(
                  val => `
                <span class="filter-item c-filter__button" data-filter-key="${filterKey}" data-filter-value="${val}">${val}</span>
              `
                )
                .join("")}
            </div>
            <div style="display: flex; flex-direction: column; gap: 4px;">
              ${col2
                .map(
                  val => `
                <span class="filter-item c-filter__button" data-filter-key="${filterKey}" data-filter-value="${val}">${val}</span>
              `
                )
                .join("")}
            </div>
          </div>
        </div>
      `;
    }

    // Render all filters UI (panel)
    function renderFilters(filters) {
      return `
        <div class="presets-filters-ui" style="margin-bottom: 24px;${presetsFilterPanelOpen ? "" : "display:none;"}">
          <div class="presets-filters-row" style="display: flex;  flex-wrap: wrap;">
            ${renderFilterColumn("INSTRUMENT", filters.instruments, "instrument")}
            ${renderFilterColumn("GENRE", filters.genres, "genre")}
            ${renderFilterColumn("TYPE", filters.categories, "type")}
            ${renderFilterColumn("PLUGIN", filters.plugins, "plugin")}
          </div>
        </div>
      `;
    }

    function updateFilterCount() {
      const activeFilters = document.querySelectorAll(".filter-item.active");
      const count = activeFilters.length;
      const badge = document.querySelector(".filter-count-badge");
      if (badge) {
        badge.textContent = count;
        badge.style.display = count > 0 ? "inline-block" : "none";
      }
    }
  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
  }
})();
