    } else {
      console.log('[SocialInjector] No Facebook button found.');
    }
    console.log('[SocialInjector] Removing social button container.');
    extLogin.parentNode.removeChild(extLogin);
  } else {
    console.log('[SocialInjector] No ext_login_hover container found.');
  }

