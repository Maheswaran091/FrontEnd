(function() {
  try {
    /* main variables */
    var debug = 0;
    var variation_name = "FL-T-39";

    /**LIBRARY**/
    var utils = {};

    /**
     * Helper function to wait for library to load
     * @param {function} trigger - callback function to be called when lib is loaded
     */
    function waitForLibrary(trigger) {
      var INTERVAL = 50;
      var TIMEOUT = 15000;

      var interval = setInterval(function() {
        if (typeof window["ilHelpers"] != "undefined") {
          clearInterval(interval);
          trigger(window["ilHelpers"]);
        }
      }, INTERVAL);

      setTimeout(function() {
        clearInterval(interval);
      }, TIMEOUT);
    }

    /**
     * Updates the modal content by adding selection controls to the last card.
     * If the subtitle of the second option exists and the selection element is not present,
     * this function inserts a selection div with "Remove" and "Add" options after the subtitle.
     */
    function updateModalContent () {
      var secondOptionSubtitle = document.querySelector("#exp-bundling-modal .il-modal-content__card:last-of-type .subtitle");
      var secondOptionSelection = document.querySelector(`#exp-bundling-modal .${variation_name}-selection`);
      if (secondOptionSubtitle && !secondOptionSelection) {
        secondOptionSubtitle.insertAdjacentHTML('afterend', `<div class="${variation_name}-selection"><span class="${variation_name}-remove">Remove</span><span class="${variation_name}-add">Add</span></div>`)
      }
    }

    /**
     * Update the class of the second option's selection element based on the status of the second option's input element
     * @returns {void}
     */
    function removeAddSecondOption () {
      const secondOptionInput = document.querySelector('#exp-bundling-modal .il-modal-content__card:last-of-type .il-modal-content__card__checkbox > input');
      const secondOptionStatus = secondOptionInput && secondOptionInput.checked;
      const secondOptionSelection = document.querySelector(`#exp-bundling-modal .${variation_name}-selection`);
      if (secondOptionStatus === true) {
        secondOptionSelection.classList.add('unselect');
      } else  if (secondOptionStatus === false) {
        secondOptionSelection.classList.remove('unselect');
      }
    }

    // Wait for library and initialize Variation
    waitForLibrary(function(ilHelpers) {
      utils = ilHelpers.get("lib");

      // Initialize Variation
      utils.waitForElement("body", function(bodyElm) {
        if (!bodyElm.classList.contains(variation_name)) {
          bodyElm.classList.add(variation_name);
          utils.observeSelector("#exp-bundling-modal", updateModalContent);
          utils.live('#exp-bundling-modal .il-modal-content__card:last-of-type', 'mousedown', removeAddSecondOption);
        }
      });
    });
    
  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
