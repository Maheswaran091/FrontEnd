    var playerInterval;

    function toggleAudio(event, productPosition, mp3, title) {
      event.stopPropagation();

      var obj = event.target;
      var preventLoop = 0;

      var audioObj; // Sounds page

      if (obj.classList.contains("c-product__player")) {
        audioObj = document.getElementById("audio__obj");
      } else if (!["SPAN", "INPUT"].includes(obj.tagName)) {
        // Presets & Plugins
        while (obj.tagName !== "BUTTON") {
          obj = obj.parentElement;
          preventLoop++;

          if (preventLoop > 10) {
            return false;
          }
        }

        audioObj = obj.nextElementSibling;
      } else {
        return false;
      } // Clear any player that's currently playing

      clearInterval(playerInterval);
      document.querySelectorAll(".playing").forEach(
        // (if not---then)
        function(audioIcon) {
          return audioIcon.classList.contains("general-player") || audioIcon.classList.remove("playing");
        }
      ); // General player (when applies)

      var generalPlayer = document.getElementById("player");

      if (audioObj.paused || (mp3 && audioObj.src !== encodeURI(mp3))) {
        document.querySelectorAll("audio").forEach(function(audioElem) {
          return audioElem.pause();
        });

        if (mp3) {
          audioObj.src = mp3;
        }

        var seeker, audioEnd, audioCurrent, generalAudioCurrent, generalAudioEnd, generalSeeker, generalVolume;

        if (productPosition !== null) {
          seeker = document.getElementById("audio__slider_".concat(productPosition));

          if (seeker) {
            // Hack to remove all previous attached events
            seeker.removeEventListener("input", audioPlayerSeekerEvent);
            seeker.audioObj = audioObj;
            seeker.addEventListener("input", audioPlayerSeekerEvent);
          }

          audioEnd = document.getElementById("audio__end_".concat(productPosition));
          audioCurrent = document.getElementById("audio__current_".concat(productPosition));

          if (generalPlayer) {
            generalPlayer.classList.remove("off"); // Time display

            generalAudioCurrent = document.getElementById("audio__current");
            generalAudioEnd = document.getElementById("audio__end"); // Seeker slider

            generalSeeker = document.getElementById("audio__slider");
            generalSeeker.removeEventListener("input", audioPlayerSeekerEvent);
            generalSeeker.audioObj = audioObj;
            generalSeeker.addEventListener("input", audioPlayerSeekerEvent); // Volume settings

            generalVolume = document.getElementById("audio__volume");
            generalVolume.removeEventListener("input", audioPlayerVolumeEvent);
            generalVolume.addEventListener("input", audioPlayerVolumeEvent);

            if (checkCookies()) {
              var previousVolume = Cookies.get("el-volume");
              generalVolume.value = previousVolume ? Cookies.get("el-volume") : 50;
            }

            audioObj.volume = generalVolume.value / 100;

            if (title) {
              document.getElementById("audio__title").innerText = title;
            }

            checkVolumeIcon(audioObj);
          } else {
            audioObj.volume = 0.5;
          }
        }

        return audioObj.play().then(function() {
          obj.classList.add("playing");

          if (audioEnd && seeker) {
            audioEnd.innerText = calculateTime(audioObj.duration);
            seeker.max = audioObj.duration;

            if (generalAudioEnd) {
              generalAudioEnd.innerText = audioEnd.innerText;
              generalSeeker.max = audioObj.duration;
            }
          }

          if (seeker && audioCurrent) {
            // Updates the player controllers & time
            playerInterval = setInterval(function() {
              if (audioObj.currentTime === audioObj.duration) {
                clearInterval(playerInterval);

                if (!obj.classList.contains("general-player")) {
                  obj.classList.remove("playing");
                }

                if (generalPlayer) {
                  generalPlayer.classList.add("off");
                }
              } else {
                seeker.value = audioObj.currentTime;
                seeker.style.backgroundSize = (audioObj.currentTime * 100) / audioObj.duration + "% 100%";
                audioCurrent.innerText = calculateTime(audioObj.currentTime);

                if (generalPlayer) {
                  generalSeeker.value = audioObj.currentTime;
                  generalSeeker.style.backgroundSize = (audioObj.currentTime * 100) / audioObj.duration + "% 100%";
                  generalAudioCurrent.innerText = audioCurrent.innerText;
                }
              }
            }, 500);
          }

          return true;
        });
      } else {
        audioObj.pause();
        audioObj.currentTime = 0;

        if (generalPlayer) {
          generalPlayer.classList.add("off");

          if (!obj.classList.contains("general-player")) {
            obj.classList.remove("playing");
