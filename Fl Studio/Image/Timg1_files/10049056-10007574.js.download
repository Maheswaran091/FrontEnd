    function fetchProductPrice() {
      // Wait for the price elements to be available in the DOM
      utils.waitForElement(".fl-t-17-item-price", function() {
        // Extract product IDs from the ctaLink in fl_featured_product_data
        const productIds = fl_featured_product_data.map(product => {
          const url = new URL(product.ctaLink);
          const params = new URLSearchParams(url.search);
          return params.get("ord");
        });

        productIds.forEach((productId, index) => {
          if (productId) {
            fetch(`https://support.image-line.com/api.php?call=product_description_inapp&callback=il_product_description_cb&format=1&product_id=${productId}`)
              .then(response => response.json())
              .then(data => {
                const productData = data[productId];
                if (productData) {
                  // Format price with currency symbol
                  const formatter = new Intl.NumberFormat("en-US", {
                    style: "currency",
                    currency: productData.currency,
                    maximumFractionDigits: 2,
                  });

                  const formattedPrice = formatter.format(productData.price);

                  // Update the price element in the DOM
                  const priceElements = document.querySelectorAll(".fl-t-17-item-price");
                  if (priceElements[index]) {
                    priceElements[index].textContent = formattedPrice;
                  }
                }
              })
              .catch(error => {
                console.error("Error fetching price:", error);
              });
          }
        });
      });
    }

