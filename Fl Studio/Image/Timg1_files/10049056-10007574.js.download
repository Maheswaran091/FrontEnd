    // --- Presets Filter UI overhaul ---
    // State for filter panel visibility
    let presetsFilterPanelOpen = false;

    // Utility: Get all active filters from UI
    function getActiveFilters() {
      return {
        genre: [...document.querySelectorAll('.filter-item[data-filter-key="genre"].active')].map(el => el.dataset.filterValue),
        instrument: [...document.querySelectorAll('.filter-item[data-filter-key="instrument"].active')].map(el => el.dataset.filterValue),
        type: [...document.querySelectorAll('.filter-item[data-filter-key="type"].active')].map(el => el.dataset.filterValue),
        plugin: [...document.querySelectorAll('.filter-item[data-filter-key="plugin"].active')].map(el => el.dataset.filterValue),
        free: !!document.querySelector(".free-selection-toggle.active"),
        owned: !!document.querySelector(".owned-toggle.active"),
        notOwned: !!document.querySelector(".not-owned-toggle.active"),
      };
    }

    // Utility: Get normalized dataset values for a product tile
    function getTileVals(tile) {
      return {
        genre: (tile.dataset.genre || "")
          .split(";")
          .map(v => v.toLowerCase().trim())
          .filter(Boolean),
        instrument: (tile.dataset.instrument || "")
          .split(";")
          .map(v => v.toLowerCase().trim())
          .filter(Boolean),
        type: (tile.dataset.type || "")
          .split(";")
          .map(v => v.toLowerCase().trim())
          .filter(Boolean),
        plugin: (tile.dataset.plugin || "")
          .split(";")
          .map(v => v.toLowerCase().trim())
          .filter(Boolean),
        free: tile.dataset.free === "1",
      };
    }

    // Utility: Check if a product matches the current filters
    function productMatchesFilters(tile, filters) {
      const vals = getTileVals(tile);
      // Ownership logic
      if (filters.owned && !filters.notOwned) return false; // Hide all if only owned
      if (filters.free && !vals.free) return false;

      // Helper function to check if a value matches any of the selected filters
      const matchesFilter = (value, selectedFilters) => {
        const normalizedValue = value.toLowerCase().trim();
        return selectedFilters.some(filter => {
          const normalizedFilter = filter.toLowerCase().trim();
          // Check for exact match or plural variation only when singular is selected
          return normalizedValue === normalizedFilter || normalizedFilter + "s" === normalizedValue;
        });
      };

      // Genre, Instrument, Type: AND logic
      for (const key of ["genre", "instrument", "type"]) {
        if (filters[key].length > 0) {
          const selected = filters[key].map(v => v.toLowerCase().trim());
          if (!vals[key].some(val => matchesFilter(val, selected))) return false;
        }
      }

      // Plugin: OR logic
      if (filters.plugin.length > 0) {
        const selected = filters.plugin.map(v => v.toLowerCase().trim());
        if (!vals.plugin.some(val => matchesFilter(val, selected))) return false;
      }
      return true;
    }

    // Utility: Show/hide products and update Show More button
    function updateProductVisibility(products, showMoreBtn, initialCount = 12) {
      const filters = getActiveFilters();
      const filtered = products.filter(tile => productMatchesFilters(tile, filters));
      // Hide all first
      products.forEach(tile => (tile.style.display = "none"));
      // Show first N filtered
      filtered.slice(0, initialCount).forEach(tile => (tile.style.display = "block"));
      // Show More button
      if (showMoreBtn) showMoreBtn.style.display = filtered.length > initialCount ? "block" : "none";
      // No results message
      const noResultsMsg = document.getElementById("no-results-message");
      if (noResultsMsg) noResultsMsg.style.display = filtered.length === 0 ? "block" : "none";
    }

    // Utility: Show all filtered products (for Show More)
    function showAllFilteredProducts(products) {
      const filters = getActiveFilters();
      const filtered = products.filter(tile => productMatchesFilters(tile, filters));
      products.forEach(tile => (tile.style.display = "none"));
      filtered.forEach(tile => (tile.style.display = "block"));
      const showMoreBtn = document.querySelector(".loadmoreBtn2");
      if (showMoreBtn) showMoreBtn.style.display = "none";
    }

