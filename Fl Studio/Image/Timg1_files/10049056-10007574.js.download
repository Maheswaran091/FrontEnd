    // Utility: Get unique filter values from products
    function getUniqueFilterValues(products) {
      const genres = new Set();
      const instruments = new Set();
      const categories = new Set();
      const plugins = new Set();
      products.forEach(product => {
        if (product.genre) {
          product.genre.split(";").forEach(g => genres.add(g.trim()));
        }
        if (product.instrument) {
          product.instrument.split(";").forEach(i => instruments.add(i.trim()));
        }
        if (product.category) {
          categories.add(product.category.trim());
        }
        if (product.plugin) {
          product.plugin.split(";").forEach(p => plugins.add(p.trim()));
        }
      });
      return {
        genres: Array.from(genres).sort(),
        instruments: Array.from(instruments).sort(),
        categories: Array.from(categories).sort(),
        plugins: Array.from(plugins).sort(),
      };
    }

