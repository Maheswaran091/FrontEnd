        <div class="fl-t-44-thumbnail" data-video="dpYGB6EUSPI" data-title="Arranging in the Playlist" data-duration="4:37">
          <img src="https://img.youtube.com/vi/dpYGB6EUSPI/0.jpg" alt="Arranging in the Playlist" class="fl-t-44-thumbnail-img">
          <div class="fl-t-44-thumb-title"><span>5.</span> Arranging in the Playlist</div>
          <div class="fl-t-44-thumb-duration">4:37</div>
        </div>
      </div>
      <div class="fl-t-44-buy-section">
        <a href='https://www.image-line.com/fl-studio/buy-now/' class="fl-t-44-buy-btn">Buy FL Studio now </a>
      </div>
    </div>`;

    function init() {
      if (!document.querySelector('.fl-t-44-container')) {
        document.querySelector('[data-heading-name="Getting started"]').insertAdjacentHTML('afterend', gettingStartedTutorial)
        eventHandler();
      }
    }

    function eventHandler() {
      const mainVideoWrapper = document.querySelector('.fl-t-44-main-video-wrapper');
      let mainVideo = null;
      const mainTitle = document.querySelector('.fl-t-44-main-video-title');
      const mainDuration = document.querySelector('.fl-t-44-main-video-duration');
      const thumbnailSelector = '.fl-t-44-thumbnail';
      const activeClass = 'fl-t-44-active';

      // Handle click on main video placeholder (first load)
      const mainVideoPlaceholder = document.getElementById('fl-t-44-main-video-placeholder');
      if (mainVideoPlaceholder) {
        mainVideoPlaceholder.addEventListener('click', function () {
          // Replace placeholder with iframe
          mainVideoWrapper.innerHTML = `<iframe id="fl-t-44-main-video" class="fl-t-44-main-video" width="800" height="450" src="https://www.youtube.com/embed/S6PSYkXmKnY?autoplay=1&mute=1&controls=1&rel=0&modestbranding=1&iv_load_policy=3&fs=0&disablekb=1" frameborder="0" allowfullscreen></iframe>`;
          mainVideo = document.getElementById('fl-t-44-main-video');
        }, { once: true });
      }

      utils.live(thumbnailSelector, 'click', function () {
        // Remove active from all thumbnails
        document.querySelectorAll(thumbnailSelector).forEach(t => t.classList.remove(activeClass));
        this.classList.add(activeClass);
        // Update main video
        const videoId = this.getAttribute('data-video');
        const title = this.getAttribute('data-title');
        const duration = this.getAttribute('data-duration');
        // If the main video is not present (i.e., still placeholder), replace it
        if (!document.getElementById('fl-t-44-main-video')) {
          mainVideoWrapper.innerHTML = `<iframe id="fl-t-44-main-video" class="fl-t-44-main-video" width="800" height="450" src="https://www.youtube.com/embed/${videoId}?autoplay=1&mute=1&controls=1&rel=0&modestbranding=1&iv_load_policy=3&fs=0&disablekb=1" frameborder="0" allowfullscreen></iframe>`;
        } else {
          document.getElementById('fl-t-44-main-video').src = `https://www.youtube.com/embed/${videoId}?autoplay=1&mute=1&controls=1&rel=0&modestbranding=1&iv_load_policy=3&fs=0&disablekb=1`;
        }
        if (mainTitle) mainTitle.textContent = title;
        if (mainDuration) mainDuration.textContent = duration;
      });
    }

    // Wait for library and initialize experiment
    waitForLibrary(function (ilHelpers) {
      utils = ilHelpers.get("lib");

      // Adding data-attributes based on the section heading
      utils.waitForElement(".wp-block-group > .wp-block-group h2", function () {
        document.querySelectorAll('.wp-block-group > .wp-block-group h2').forEach(h2 => {
          const headingText = h2.textContent.trim();
          const grandParent = h2.parentElement?.parentElement;
          if (grandParent) {
            grandParent.setAttribute('data-heading-name', headingText);
          }
        });
      });

      utils.waitForElement("body.signed-in, body.signed-out", function (bodyElm) {

        if (!bodyElm.classList.contains(variation_name)) {
          bodyElm.classList.add(variation_name);
        }

        utils.waitForElement('[data-heading-name="Getting started"]', init);
      });
    });

  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
