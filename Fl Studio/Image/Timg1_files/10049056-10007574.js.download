(function() {
  try {
    /* main variables */
    var debug = 0;
    var variation_name = "fl-t-40";

    /**LIBRARY**/
    var utils = {};

    /**
     * Helper function to wait for library to load
     * @param {function} trigger - callback function to be called when lib is loaded
     */
    function waitForLibrary(trigger) {
      var INTERVAL = 50;
      var TIMEOUT = 15000;

      var interval = setInterval(function() {
        if (typeof window["ilHelpers"] != "undefined") {
          clearInterval(interval);
          trigger(window["ilHelpers"]);
        }
      }, INTERVAL);

      setTimeout(function() {
        clearInterval(interval);
      }, TIMEOUT);
    }

    function init() {
      var siteHeaderLogo = document.querySelector('.site-header__logo');
      siteHeaderLogo && (siteHeaderLogo.innerHTML = '');
    }

    function moveLoginButton() {
      const accountInfo = document.querySelector('nav + div');
      if(accountInfo.classList.contains('site-header-meta')) {
        utils.waitForElement('.site-header-meta a.sign-in-button.visually-visible', function(){
          var loginUrl = document.querySelector('.site-header-meta a.sign-in-button.visually-visible');
          const mobileNavSection = document.querySelector('.nav-mobile__sections');
          mobileNavSection.insertAdjacentHTML('afterbegin', `<li class="nav-mobile__section"><div class="nav-mobile__label__wrapper"><div class="nav-mobile__label">${accountInfo.outerHTML}</div></div></li>`);
        });
      } else if (accountInfo.classList.contains('c-header-shop')) {
        var navTitle = document.querySelector('aside .c-nav__parent-title');
        var subMenu = document.querySelector('aside a[href="https://www.image-line.com/fl-studio"] + .sub-menu');
        if(navTitle) {
          navTitle && navTitle.insertAdjacentElement('afterend', document.querySelector('.c-header-shop'));
        } else if (subMenu) {
          subMenu && subMenu.insertAdjacentElement('beforebegin', document.querySelector('.c-header-shop'));
        }
      }

      var headerNav = document.querySelector('header nav');
      headerNav && !document.querySelector(`.${variation_name}-buy-fl-stdio`) && headerNav.insertAdjacentHTML('afterend', `<a href="https://www.image-line.com/fl-studio" class="${variation_name}-buy-fl-stdio">Buy FL Studio</a>`);
    }


    // Wait for library and initialize experiment
    waitForLibrary(function(ilHelpers) {
      utils = ilHelpers.get("lib");

      utils.waitForElement("body", function(bodyElm) {
        if (!bodyElm.classList.contains(variation_name)) {
          bodyElm.classList.add(variation_name);
          utils.waitForElement('.site-header__logo', init);
          utils.waitForElement('header nav a', moveLoginButton);
          if(window.location.hostname == 'support.image-line.com') {
            bodyElm.classList.add(variation_name + '--support');
          }
        }
      });
    });
  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
  }
})();
