    // âœ… Updated to fallback using forex if currency not directly found
    function getProductPrice(product, userSettings, symbol) {
      const { currency, forex } = userSettings;
      const priceInfo = product.productProductPrices.find(p => p.currency === currency);

      if (priceInfo) {
        return priceInfo.price;
      } else {
        // Fallback: Try to convert from USD
        const usdInfo = product.productProductPrices.find(p => p.currency === "USD");
        if (usdInfo && forex?.[currency]) {
          const converted = parseFloat(usdInfo.price) * parseFloat(forex[currency]);
          return converted.toFixed(2);
        } else {
          // Last resort fallback to first available price
          return product.productProductPrices[0]?.price || "N/A";
        }
      }
    }

