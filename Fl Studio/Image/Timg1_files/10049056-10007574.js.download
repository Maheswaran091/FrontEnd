          utils.waitForElement("#buy-now-header > .buy-now-columns", init);
          utils.waitForElement("#buy-now-header > .buy-now-columns", insertNewHeader);
        });
      }
    });
  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
  }
})();
