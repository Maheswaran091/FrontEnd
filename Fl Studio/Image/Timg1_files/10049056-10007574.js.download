    function loadMoreProducts(containerSelector, productSelector, buttonSelector, initialCount = 12) {
      const checkExist = setInterval(() => {
        const container = document.querySelector(containerSelector);
        const button = document.querySelector(buttonSelector);

        if (container && button) {
          clearInterval(checkExist);
          const products = container.querySelectorAll(productSelector);

          // Get active filters
          const activeFilters = {
            genre: [...document.querySelectorAll('.filter-item[data-filter-key="genre"].active')].map(el => el.dataset.filterValue),
            instrument: [...document.querySelectorAll('.filter-item[data-filter-key="instrument"].active')].map(el => el.dataset.filterValue),
            type: [...document.querySelectorAll('.filter-item[data-filter-key="type"].active')].map(el => el.dataset.filterValue),
            plugin: [...document.querySelectorAll('.filter-item[data-filter-key="plugin"].active')].map(el => el.dataset.filterValue),
            free: document.querySelector(".free-selection-toggle.active") ? true : false,
          };

          // Check if any filters are active
          const hasActiveFilters =
            Object.values(activeFilters).some(filters => filters.length > 0) || document.querySelector(".owned-toggle.active") || document.querySelector(".not-owned-toggle.active");

          if (!hasActiveFilters) {
            // No filters active: show first N products
            products.forEach((product, index) => {
              product.style.display = index < initialCount ? "block" : "none";
            });
          } else {
            // Filters active: show first N matching products
            const visibleProducts = Array.from(products).filter(p => {
              const tileVals = {
                genre: (p.dataset.genre || "")
                  .split(";")
                  .map(v => v.trim())
                  .filter(Boolean),
                instrument: (p.dataset.instrument || "")
                  .split(";")
                  .map(v => v.trim())
                  .filter(Boolean),
                type: (p.dataset.type || "")
                  .split(";")
                  .map(v => v.trim())
                  .filter(Boolean),
                plugin: (p.dataset.plugin || "")
                  .split(";")
                  .map(v => v.trim())
                  .filter(Boolean),
              };

              // Check ownership filters
              const ownedBtn = document.querySelector(".owned-toggle");
              const notOwnedBtn = document.querySelector(".not-owned-toggle");
              const ownedActive = ownedBtn && ownedBtn.classList.contains("active");
              const notOwnedActive = notOwnedBtn && notOwnedBtn.classList.contains("active");

              if (ownedActive && !notOwnedActive) return false;
              if (activeFilters.free && p.dataset.free !== "1") return false;

              // Check other filters
              for (const key of ["genre", "instrument", "type", "plugin"]) {
                if (activeFilters[key].length > 0) {
                  if (key === "plugin") {
                    if (!activeFilters[key].some(val => tileVals[key].includes(val))) return false;
                  } else if (key === "type") {
                    const normalizedTileVals = tileVals[key].map(v => v.toLowerCase().trim());
                    const normalizedActiveVals = activeFilters[key].map(v => v.toLowerCase().trim());
                    if (!normalizedActiveVals.every(val => normalizedTileVals.includes(val))) return false;
                  } else {
                    if (!activeFilters[key].every(val => tileVals[key].includes(val))) return false;
                  }
                }
              }

              return true;
            });

            // Show first N matching products
            products.forEach((product, index) => {
              product.style.display = visibleProducts.includes(product) && index < initialCount ? "block" : "none";
            });
          }

          utils.live(buttonSelector, "click", function(event) {
            // When "Show More" is clicked, show all matching products
            if (hasActiveFilters) {
              const matchingProducts = Array.from(products).filter(p => {
                const tileVals = {
                  genre: (p.dataset.genre || "")
                    .split(";")
                    .map(v => v.trim())
                    .filter(Boolean),
                  instrument: (p.dataset.instrument || "")
                    .split(";")
                    .map(v => v.trim())
                    .filter(Boolean),
                  type: (p.dataset.type || "")
                    .split(";")
                    .map(v => v.trim())
                    .filter(Boolean),
                  plugin: (p.dataset.plugin || "")
                    .split(";")
                    .map(v => v.trim())
                    .filter(Boolean),
                };

                // Check ownership filters
                const ownedBtn = document.querySelector(".owned-toggle");
                const notOwnedBtn = document.querySelector(".not-owned-toggle");
                const ownedActive = ownedBtn && ownedBtn.classList.contains("active");
                const notOwnedActive = notOwnedBtn && notOwnedBtn.classList.contains("active");

                if (ownedActive && !notOwnedActive) return false;
                if (activeFilters.free && p.dataset.free !== "1") return false;

                // Check other filters
                for (const key of ["genre", "instrument", "type", "plugin"]) {
                  if (activeFilters[key].length > 0) {
                    if (key === "plugin") {
                      if (!activeFilters[key].some(val => tileVals[key].includes(val))) return false;
                    } else if (key === "type") {
                      const normalizedTileVals = tileVals[key].map(v => v.toLowerCase().trim());
                      const normalizedActiveVals = activeFilters[key].map(v => v.toLowerCase().trim());
                      if (!normalizedActiveVals.every(val => normalizedTileVals.includes(val))) return false;
                    } else {
                      if (!activeFilters[key].every(val => tileVals[key].includes(val))) return false;
                    }
                  }
                }

                return true;
              });

              matchingProducts.forEach(p => (p.style.display = "block"));
            } else {
              // No filters active: show all products
              products.forEach(p => (p.style.display = "block"));
            }
            button.style.display = "none";
          });
        }
      }, 200);

      setTimeout(() => {
        clearInterval(checkExist);
      }, 5000);
    }

    var playerInterval;

    function toggleAudio(event, productPosition, mp3, title) {
      event.stopPropagation();

      var obj = event.target;
      var preventLoop = 0;

      var audioObj; // Sounds page

      if (obj.classList.contains("c-product__player")) {
        audioObj = document.getElementById("audio__obj");
      } else if (!["SPAN", "INPUT"].includes(obj.tagName)) {
        // Presets & Plugins
        while (obj.tagName !== "BUTTON") {
          obj = obj.parentElement;
          preventLoop++;

          if (preventLoop > 10) {
            return false;
          }
        }

        audioObj = obj.nextElementSibling;
      } else {
        return false;
      } // Clear any player that's currently playing

      clearInterval(playerInterval);
      document.querySelectorAll(".playing").forEach(
        // (if not---then)
        function(audioIcon) {
          return audioIcon.classList.contains("general-player") || audioIcon.classList.remove("playing");
        }
      ); // General player (when applies)

      var generalPlayer = document.getElementById("player");

      if (audioObj.paused || (mp3 && audioObj.src !== encodeURI(mp3))) {
        document.querySelectorAll("audio").forEach(function(audioElem) {
          return audioElem.pause();
        });

        if (mp3) {
          audioObj.src = mp3;
        }

        var seeker, audioEnd, audioCurrent, generalAudioCurrent, generalAudioEnd, generalSeeker, generalVolume;

        if (productPosition !== null) {
          seeker = document.getElementById("audio__slider_".concat(productPosition));

          if (seeker) {
            // Hack to remove all previous attached events
            seeker.removeEventListener("input", audioPlayerSeekerEvent);
            seeker.audioObj = audioObj;
            seeker.addEventListener("input", audioPlayerSeekerEvent);
          }

          audioEnd = document.getElementById("audio__end_".concat(productPosition));
          audioCurrent = document.getElementById("audio__current_".concat(productPosition));

          if (generalPlayer) {
            generalPlayer.classList.remove("off"); // Time display

            generalAudioCurrent = document.getElementById("audio__current");
            generalAudioEnd = document.getElementById("audio__end"); // Seeker slider

            generalSeeker = document.getElementById("audio__slider");
            generalSeeker.removeEventListener("input", audioPlayerSeekerEvent);
            generalSeeker.audioObj = audioObj;
            generalSeeker.addEventListener("input", audioPlayerSeekerEvent); // Volume settings

            generalVolume = document.getElementById("audio__volume");
            generalVolume.removeEventListener("input", audioPlayerVolumeEvent);
            generalVolume.addEventListener("input", audioPlayerVolumeEvent);

            if (checkCookies()) {
              var previousVolume = Cookies.get("el-volume");
              generalVolume.value = previousVolume ? Cookies.get("el-volume") : 50;
            }

            audioObj.volume = generalVolume.value / 100;

            if (title) {
              document.getElementById("audio__title").innerText = title;
            }

            checkVolumeIcon(audioObj);
          } else {
            audioObj.volume = 0.5;
          }
        }

        return audioObj.play().then(function() {
          obj.classList.add("playing");

          if (audioEnd && seeker) {
            audioEnd.innerText = calculateTime(audioObj.duration);
            seeker.max = audioObj.duration;

            if (generalAudioEnd) {
              generalAudioEnd.innerText = audioEnd.innerText;
              generalSeeker.max = audioObj.duration;
            }
          }

          if (seeker && audioCurrent) {
            // Updates the player controllers & time
            playerInterval = setInterval(function() {
              if (audioObj.currentTime === audioObj.duration) {
                clearInterval(playerInterval);

                if (!obj.classList.contains("general-player")) {
                  obj.classList.remove("playing");
                }

                if (generalPlayer) {
                  generalPlayer.classList.add("off");
                }
              } else {
                seeker.value = audioObj.currentTime;
                seeker.style.backgroundSize = (audioObj.currentTime * 100) / audioObj.duration + "% 100%";
                audioCurrent.innerText = calculateTime(audioObj.currentTime);

                if (generalPlayer) {
                  generalSeeker.value = audioObj.currentTime;
                  generalSeeker.style.backgroundSize = (audioObj.currentTime * 100) / audioObj.duration + "% 100%";
                  generalAudioCurrent.innerText = audioCurrent.innerText;
                }
              }
            }, 500);
          }

          return true;
        });
      } else {
        audioObj.pause();
        audioObj.currentTime = 0;

        if (generalPlayer) {
          generalPlayer.classList.add("off");

          if (!obj.classList.contains("general-player")) {
            obj.classList.remove("playing");
          }
        }
      }
    }

