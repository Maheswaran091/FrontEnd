    // Helper function to create individual artist card HTML
    function createArtistCard(artist) {
      return `
          <div class="${variation_name}-artist-card" data-genre-key="${artist.Genre}">
              <div class="${variation_name}-artist-content">
                  <img src="${artist["Alt image"] || artist["Image"]}" alt="${artist.Name}">
