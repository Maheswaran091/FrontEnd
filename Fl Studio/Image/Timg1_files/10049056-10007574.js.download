(function () {
  try {
    /* main variables */
    var debug = 0;
    var variation_name = "fl-t-42";

    /**LIBRARY**/
    var utils = {};

    /**
     * Helper function to wait for library to load
     * @param {function} trigger - callback function to be called when lib is loaded
     */
    function waitForLibrary(trigger) {
      var INTERVAL = 50;
      var TIMEOUT = 20000;

      var interval = setInterval(function () {
        console.log(typeof window["ilHelpers"] != "undefined");
        if (typeof window["ilHelpers"] != "undefined") {
          clearInterval(interval);
          trigger(window["ilHelpers"]);
        }
      }, INTERVAL);

      setTimeout(function () {
        clearInterval(interval);
      }, TIMEOUT);
    }

    var detectedRegion = "US"; // fallback
    var regionProducers = {
      Europe: [
        "Martin Garrix",
        "Hit-Boy",
        "Boi-1da",
        "Afrojack",
        "Mustard",
        "Ovy On The Drums"
      ],
      US: [
        "Boi-1da",
        "Martin Garrix",
        "Mustard",
        "Afrojack",
        "Hit-Boy",
        "Ovy On The Drums"
      ],
      Africa: [
        "Boi-1da",
        "P.Priime",
        "Ovy On The Drums",
        "Hit-Boy",
        "Martin Garrix",
        "Kel-P",
      ],
      "Lat America": [
        "Ovy On The Drums",
        "Demy & Clipz",
        "Boi-1da",
        "Hit-Boy",
        "Martin Garrix",
        "Nely (El Arma Secreta)"
      ],
      Asia: [
        "Martin Garrix",
        "Hit-Boy",
        "Mustard",
        "Karan Kanchan",
        "Porter Robinson",
        "Ovy On The Drums"
      ]
    };


    /**
     * Extracts the allArtists array from the DOM (from .js-artist-listing[data-artists])
     * Returns an array of artist objects, or [] if not found/parse error.
     */

    function getAllArtistsFromDOM() {
      var dataSourceEl = document.querySelector(".js-artist-listing");
      if (!dataSourceEl) return [];
      var raw = dataSourceEl.getAttribute("data-artists");
      if (!raw) return [];
      var textarea = document.createElement("textarea");
      textarea.innerHTML = raw;
      try {
        return JSON.parse(textarea.value);
      } catch (e) {
        if (debug) console.error(" Artist JSON parsing failed:", e);
        return [];
      }
    }
        function getRegion(callback) {
      try {
        if (!window.convert || typeof window.convert.getUserData !== "function") {
          throw new Error("Convert or getUserData not available");
        }

        var userLocation = window.convert.getUserData();
        if (!userLocation || !userLocation.geo) {
          throw new Error("Geo data not found in userLocation");
        }

        var continent = userLocation.geo.continent || "";
        var countryCode = userLocation.geo.country || "";
        var region = "";

        switch (continent) {
          case "EU":
            region = "Europe";
            break;
          case "NA":
            region = countryCode === "US" ? "US" : "Lat America";
            break;
          case "SA":
            region = "Lat America";
            break;
          case "AF":
            region = "Africa";
            break;
          case "AS":
            region = "Asia";
            break;
          default:
            region = "Europe";
        }

        // Fallback if region not supported in regionProducers
        if (typeof regionProducers !== "undefined" && !regionProducers[region]) {
          region = "Europe";
        }

        // Set global if needed
        detectedRegion = region;

        // Add region class safely
        document.body.classList.add("region-" + region.toLowerCase().replace(/\s+/g, "-"));

        // Screen width class
        if (window.innerWidth > 1920) {
          document.body.classList.add("screen-large");
        }

        if (typeof debug !== "undefined" && debug) {
          console.log("ðŸŒ Region detected:", region);
        }

        callback(region);
      } catch (e) {
        // Fallback region
        detectedRegion = "Europe";
        document.body.classList.add("region-europe");

        if (typeof debug !== "undefined" && debug) {
          console.log("ðŸŒ Region fallback applied due to error:", e);
        }

        callback("Europe");
      }
    }

    function renderArtistsForRegion(region, allArtists) {
      var container = document.getElementById("artistContainer");
      var template = document.getElementById("templateCard");
      if (!container || !template) return;
      container.innerHTML = "";
      container.appendChild(template);
      (regionProducers[region] || []).forEach(function (name) {
        var artist = allArtists.find(a => a.name === name);
        if (!artist) return;
        var card = template.cloneNode(true);
        card.id = "";
        card.style.display = "";
        var img = card.querySelector(".artist-image");
        if (img) {
          img.src = artist.image;
          img.alt = "Photo of " + artist.name;
        }
        var nameEl = card.querySelector(".artist-name");
        if (nameEl) nameEl.textContent = artist.name;
        var bio = card.querySelector(".artist-bio");
        if (bio) bio.innerHTML = artist.bio;
        var quote = card.querySelector(".quote-text");
        if (quote) quote.textContent = artist.quote;
        container.appendChild(card);
      });
    }
    function applyHeroImage(region) {
      var isLargeScreen = window.innerWidth > 1920;
      var regionClass = "region-" + region.toLowerCase();
      document.body.classList.add(regionClass);
      // if(debug) console.log(" Body classes set for:",regionClass,"| screen-large:",isLargeScreen);
    }
    // Initial point
    function init() {
      // Example usage: get all artists from DOM
      var allArtists = getAllArtistsFromDOM();
      // console.log(allArtists, 'allArtists');

      if (debug && allArtists.length > 0) {
        console.log("First artist:", allArtists[0]);
      }

      // Region-based artist rendering logic
      utils.waitForElement("#artistContainer #templateCard", function () {
        getRegion(function (region) {
          applyHeroImage(region);
          renderArtistsForRegion(region, allArtists);
        });
      });
    }

    function eventHandlers() {
      // utils.live("body", "click", function() {
      //   console.log("body click");
      // });
    }

    /**
     * Library ready entrypoint
     */
    waitForLibrary(function (ilHelpers) {
      utils = ilHelpers.get("lib");
      console.log("library loaded", utils);

      // Wait for body element and mark variation
      utils.waitForElement("body.signed-in, body.signed-out", function (bodyElm) {
        if (!bodyElm.classList.contains(variation_name)) {
          bodyElm.classList.add(variation_name);
          eventHandlers();
        }

        utils.waitForElement("body", init);
      });
    });
  } catch (e) {
    if (debug) console.log(e, "error in Test" + variation_name);
  }
})();
